      "id": "kcnsscw1cmsz",
      "name": "Subsea Fiber Faults in the APAC region",
      "status": "identified",
      "created_at": "2023-01-29T16:27:11.420Z",
      "updated_at": "2023-03-29T13:40:59.351Z",
